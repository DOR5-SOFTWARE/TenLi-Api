        "gcServer": true
    },

    "runtimes": {
        "ubuntu.14.04-x64": {},
        "win8-x64": {},
        "win10-x64": {},
        "osx.10.11-x64": {}
    },

    "publishOptions": {
        "include": [
            "appsettings.json",
            "web.config"
        ]
    }
}